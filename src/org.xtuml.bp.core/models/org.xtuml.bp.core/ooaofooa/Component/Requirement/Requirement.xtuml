INSERT INTO O_TFR
	VALUES ("375735e2-f1a6-4173-804e-740217547201",
	"74121abd-8a97-4ef3-a0af-5cea170b182b",
	'synchronize',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'/**
