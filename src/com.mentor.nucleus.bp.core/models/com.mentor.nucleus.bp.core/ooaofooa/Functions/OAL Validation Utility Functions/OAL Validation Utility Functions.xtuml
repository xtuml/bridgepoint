    select one val related by arrayVal->V_VAL[R801];
    relate val to dt across R820;
  else
   atEnd = true;
  end if;
end while; // Iteration complete
if not_empty val
  return val.Value_ID;
else
  return GD::NULL_UNIQUE_ID();
end if;
